#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/@tauri-apps+cli@1.5.14/node_modules/@tauri-apps/cli/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/@tauri-apps+cli@1.5.14/node_modules/@tauri-apps/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/@tauri-apps+cli@1.5.14/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/@tauri-apps+cli@1.5.14/node_modules/@tauri-apps/cli/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/@tauri-apps+cli@1.5.14/node_modules/@tauri-apps/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/@tauri-apps+cli@1.5.14/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize/Build/Vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../tauri.js" "$@"
else
  exec node  "$basedir/../../tauri.js" "$@"
fi
