#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/prettier@3.3.2/node_modules/prettier/bin/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/prettier@3.3.2/node_modules/prettier/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/prettier@3.3.2/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/prettier@3.3.2/node_modules/prettier/bin/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/prettier@3.3.2/node_modules/prettier/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/prettier@3.3.2/node_modules:/Users/yyx/Library/CloudStorage/OneDrive-琉球大学/code/unity/games101_rasterize_by_unity_webgl/Build/Vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../prettier/bin/prettier.cjs" "$@"
fi
